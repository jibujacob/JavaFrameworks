package com.jj.springsecurity.demo.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class DemoController {

	@GetMapping("/")
	public String showHome() {
		
		return "home";
	}
	
	@GetMapping("/leaders")
	public String showLeadersPage() {
		
		return "showLeadersPage";
	}
	
	@GetMapping("/systems")
	public String showLSystemsPage() {
		
		return "showLSystemsPage";
	}
}
